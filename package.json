{
  "name": "react-fix-it",
  "version": "0.3.1",
  "description": "Automagically generate tests from errors",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/MicheleBertoli/react-fix-it.git",
  "author": "Michele Bertoli",
  "license": "MIT",
  "scripts": {
